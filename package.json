{
  "name": "@vcita/event-bus-nestjs",
  "version": "1.0.4",
  "description": "Event Bus for NestJS applications with AMQP support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "preversion": "npm run lint && npm run build && npm test",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "1.20.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@vcita/infra-nestjs": "^2.23.1",
    "@vcita/oauth-client-nestjs": "3.x || 4.x"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.15",
    "@types/amqplib": "^0.10.1",
    "@types/jest": "^29.5.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vcita/eslint-config-nestjs": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vcita/event-bus-nestjs.git"
  },
  "keywords": [
    "vcita",
    "event-bus",
    "nestjs",
    "amqp",
    "rabbitmq"
  ],
  "author": "VCita",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vcita/event-bus-nestjs/issues"
  },
  "homepage": "https://github.com/vcita/event-bus-nestjs#readme"
}
